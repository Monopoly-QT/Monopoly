{
    "move": {
        "name": "/move",
        "description": "Move to a specific position on the board.",
        "usage": "/move [location] or /move to [0-31]",
        "examples": [
            "/move 5",
            "/move to 10"
        ],
        "prompt": "Moving {playerName} to position {location}."
    },
    "give": {
        "name": "/give",
        "description": "Give money to another player.",
        "usage": "/give [playerName] [money]",
        "examples": [
            "/give Tuan 500",
            "/give Wu 500",
            "/give Chang 500",
            "/give Chiang 500"
        ],
        "prompt": "{playerName} received {money} dollars from you."
    },
    "get": {
        "name": "/get",
        "description": "Get money from the system.",
        "usage": "/get [money]",
        "examples": [
            "/get 100"
        ],
        "prompt": "{playerName} received {money} dollars from the system."
    },
    "card": {
        "name": "/card",
        "description": "Retrieve a specific card by name.",
        "usage": "/card [card_name]",
        "examples": [
            "/card Lucky",
            "/card Rocket",
            "/card RoadBlock",
            "/card RandomEvent",
            "/card DeHouse"
        ],
        "prompt": "You received the card: {card_name}."
    },
    "minigame": {
        "name": "/minigame",
        "description": "Enter a minigame.",
        "usage": "/minigame [minigame_name]",
        "examples": [
            "/minigame HorseRacing",
            "/minigame InBetween"
        ],
        "prompt": "Entering the minigame [minigame_name]."
    },
    "gamestate": {
        "name": "/gamestate",
        "description": "Change the game state.",
        "usage": "/gamestate [state]",
        "examples": [
            "/gamestate INIT",
            "/gamestate START",
            "/gamestate MOVED",
            "/gamestate FINISH"
        ],
        "prompt": "Game state changed to {state}."
    },
    "info": {
        "name": "/info",
        "description": "Display information about all players.",
        "usage": "/info",
        "examples": [
            "/info"
        ],
        "prompt": "Displaying information for all players."
    },
    "refresh": {
        "name": "/refresh",
        "description": "Refresh the game board.",
        "usage": "/refresh",
        "examples": [
            "/refresh"
        ],
        "prompt": "The game board has been refreshed."
    },
    "list": {
        "name": "/list",
        "description": "List all available commands.",
        "usage": "/list",
        "examples": [
            "/list"
        ],
        "prompt": "Available commands:"
    },
    "help": {
        "name": "/help",
        "description": "The helper of all available commands.",
        "usage": "/help",
        "examples": [
            "/help"
        ],
        "prompt": "Available commands:"
    },
    "invalid_command": {
        "name": "",
        "prompt": "Invalid command. \nType \"/list\" or \"/help\" to see brief infomation of available commands.\nType \"/list -a\" or \"/help -a\" to see details of available commands."
    }
}
